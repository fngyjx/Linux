CREATE DEFINER=`csheaff`@`%` PROCEDURE `CreatePendingReceipt`( lot_number VARCHAR(11), lot_sequence INT(10), po_number INT(10), po_sequence SMALLINT(5), quantity DOUBLE(15,5), packsize DOUBLE(15,5), units VARCHAR(5), OUT LARID INT(11) )
BEGIN
	DECLARE IMTN INT(10);
	DECLARE product_number VARCHAR(20);
	DECLARE EXIT HANDLER FOR SQLEXCEPTION, SQLWARNING, NOT FOUND
	BEGIN
	  ROLLBACK;
	END;
	START TRANSACTION;
		INSERT INTO lots_and_receipts (LotNumber, LotSequenceNumber, PurchaseOrderNumber, PurchaseOrderSeqNumber, Status, Quantity, PackSize) VALUES ( lot_number, lot_sequence, po_number, po_sequence, 'P', quantity, packsize );
		SELECT LAST_INSERT_ID() INTO LARID;
		SELECT ProductNumberInternal INTO product_number FROM purchaseorderdetail WHERE PurchaseOrderNumber=po_number AND PurchaseOrderSeqNumber=po_sequence;
		INSERT INTO inventorymovements (LotNumber, LotSequenceNumber, ProductnumberInternal, TransactionDate, Quantity, TransactionType, MovementStatus) VALUES ( lot_number, lot_sequence, product_number, CURDATE(), QuantityConvert(quantity*packsize, units, 'grams'), 1, 'P' );
		SELECT LAST_INSERT_ID() INTO IMTN;
		UPDATE lots_and_receipts SET InventoryMovementTransactionNumber = IMTN WHERE ID=LARID;
	COMMIT;
END$$

